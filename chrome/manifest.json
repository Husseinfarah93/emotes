{
  "name": "emotes",
  "description": "emotes for the web",
  "version": "1.0",
  "incognito": "split",
  "content_scripts": [
    {
      "matches": ["http://*/*","https://*/*"],
      "js": ["runme.js"],
      "run_at": "document_end"
    }
  ],
  "browser_action": {
   "default_icon": "./static/pogchamp.jpg",
   "default_popup": "app.html"
  },
  "permissions": [
   "tabs"
   ],
  "manifest_version": 2
}
  /*
  ViewPort 
    Navbar Container 
    SearchBar Container 
    Emojis Container   
  */